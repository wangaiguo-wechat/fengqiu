/* pages/home/home.wxss */

.header {
    width: 750rpx;
    height: 60px;
    display: flex;
}
.header .left{
    margin: 10px;
    width: 45%;
    height: 40px;
    float: left;
}
.header .right{
    margin: 10px;
    width: 45%;
    height: 40px;
    float: right;
}
 .pic {
    width: 40px;
    height: 40px;
    float: left;
}
 .contentBack{
    margin-left: 10px;
    float: left;
    width: 100px;
    height: 40px;
}
.contentBack .title{
    color: black;
    font-size: 17px;
    height: 20px;
    width: auto;
}
.contentBack .subTitle{
    margin-top: 5px;
    color: gray;
    font-size: 13px;
    height: 20px;
    width: auto;
}